<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoadingIndicatorView">
        <attr name="indicator">
            <flag name="BallPulse" value="0"/>
            <flag name="BallSpinFadeLoader" value="1"/>
        </attr>
        <attr name="indicator_color" format="color"/>
        <attr name="indicator_size" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SwipeMenuLayout">
        <attr name="leftViewId" format="reference" />
        <attr name="rightViewId" format="reference" />
        <attr name="contentViewId" format="reference" />
    </declare-styleable>

</resources>